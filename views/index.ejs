<!DOCTYPE html>
<html>

<head>

  <link rel='stylesheet' href='/stylesheets/style.css' />
  <script src="https://code.jquery.com/jquery-3.5.1.min.js"
    integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
  <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
  <link rel="stylesheet" href="/resources/demos/style.css">
  <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
  <style>
    tr {
      padding: 10px;
    }
  </style>

  <script>

  </script>
</head>

<body>
  <h1>Công Nợ Simsoviet (Đại Lý Giao)</h1>
  <form action="/cart" method="post">
  <table>
    <tr>
      <td><label for="">Đại Lý</label></td>
      <td>
        <!-- <select name="" id="">
        <% for(var i = 0;i<getAllAgency.length;i++){ %>
          <option value= "<%= getAllAgency[i]._id %>"  id=''>  <%= getAllAgency[i].name%>    </option>

        <% } %>  
        </select> -->
        <div class="ui-widget">
          <input class="tags" name="name">
        </div>

      </td>
    </tr>
    <tr>
      <td><label for="">Số Sim</label></td>
      <td><input type="text" name="sim" id=""></td>
    </tr>
    <tr>
      <td><label for="">Giá Nhập</label></td>
      <td><input type="text" name="entryPrice" id=""></td>
    </tr>
    <tr>
      <td><label for="">Giá Bán</label></td>
      <td><input type="text" name="price" id=""></td>
    </tr>
    <tr>
      <td><label for="">Hoa Hồng Đại Lý</label></td>
      <td><input type="text" name="commissionAgency" id=""></td>
    </tr>
    <tr>
      <td><label for="">Hoa Hồng Nhân Viên</label></td>
      <td><input type="text" name="commissionUser" id=""></td>
    </tr>
    <tr>
      <td><label for="">Chi Phí Giao Sim</label></td>
      <td><input type="text" name="fee" id=""></td>
    </tr>
    <tr>
      <td><label for="">Đại Lý Hỗ Trợ</label></td>
      <td><input type="text" name="agencySupport" id=""></td>
    </tr>
    <tr>
      <td><label for="">Chi phi neu that bai</label></td>
      <td><input type="text" name="feeIfFalse" id=""></td>
    </tr>
  </table>
  <button type="submit">Create Cart</button>
</form>
</body>

<script>

    $.ajax({
      url: '/agency',
      type: 'GET'
    }).then((result) => {
      var results= result.map(function(value){
        return value.name
      })
      $(".tags").autocomplete({
        source: results
      });
    }).catch((err) => {
      console.log(err);
    });
</script>

</html>