<script>
    $.ajax({
      url: '/agency',
      type: 'GET'
    }).then((result) => {
      var results= result.map(function(value){
        return value.idAgency.name
      });
      $(".agency").autocomplete({
        source: results
      });
    }).catch((err) => {
      console.log(err);
    });



    $("#addnew").click(function(data){
        var name=$(".name").val();
        var sim=$(".sim").val();
        var entryPrice=$(".entryPrice").val();
        var price=$(".price").val();
        var fee=$(".fee").val();
        var agencySupport=$(".agencySupport").val();
        var feeIfFalse=$(".feeIfFalse").val();
        $.ajax({
                url: "/cart",
                type: "post",
                data: {
                       name:name,
                       sim:sim,
                       entryPrice:entryPrice,
                       price:price,
                       fee:fee,
                       agencySupport:agencySupport,
                       feeIfFalse:feeIfFalse
                      }
              }).then(function(data){
                    if(data.error){
                    alert(data)
                  }else{
                        alert("thanhcong");
                        window.location.href = "/showagency"
                       }
                     });
                    });


                    
                    // page==================appen data





</script>